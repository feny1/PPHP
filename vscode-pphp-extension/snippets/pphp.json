{
    "WELCOMING": {
        "prefix": ["W", "WELCOMING"],
        "body": [
            "WELCOMING = $0"
        ],
        "description": "Defines a welcoming message"
    },
    "DESCRIPTION": {
        "prefix": ["D", "DESCRIPTION"],
        "body": [
            "DESCRIPTION = $0"
        ],
        "description": "Defines a description message"
    },
    "TYPE": {
        "prefix": ["T", "TYPE"],
        "body": [
            "TYPE = ${1|dashboard,welcoming,form,list|}$0"
        ],
        "description": "Defines a type"
    },
    "TITLE": {
        "prefix": ["TI", "TITLE"],
        "body": [
            "TITLE = $0"
        ],
        "description": "Defines a title"
    },
    "FEATURE": {
        "prefix": ["F", "FEATURE"],
        "body": [
            "FEATURE = $0"
        ],
        "description": "Defines a feature"
    },
    "ITEM": {
        "prefix": ["I", "ITEM"],
        "body": [
            "ITEM = $0"
        ],
        "description": "Defines an item"
    },
    "LIST": {
        "prefix": ["L", "LIST"],
        "body": [
            "<<LIST>> ${1:item1}|${2:item2}|${3:item3}$0"
        ],
        "description": "Defines a list of items"
    },
    "OBJECTS": {
        "prefix": ["OS", "OBJECTS"],
        "body": [
            "<<OBJECTS>> ${1:key1}=>${2:value1}, ${3:key2}=>${4:value2} | ${5:key3}=>${6:value3}$0"
        ],
        "description": "Defines multiple objects with properties"
    },
    "OBJECT": {
        "prefix": ["O", "OBJECT"],
        "body": [
            "<<OBJECT>> ${1:key1}=>${2:value1}, ${3:key2}=>${4:value2}$0"
        ],
        "description": "Defines a single object with properties"
    },
    "PPHP Template": {
        "prefix": ["template", "pphp"],
        "body": [
            "TYPE = ${1|dashboard,welcoming|}",
            "TITLE = ${2:My Title}",
            "WELCOMING = ${3:Welcome Message}",
            "DESCRIPTION = ${4:Description}",
            "$0"
        ],
        "description": "Creates a basic PPHP template"
    }
}
