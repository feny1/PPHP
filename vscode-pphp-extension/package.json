{
    "name": "pphp-extension",
    "displayName": "PPHP Support",
    "description": "Provides syntax highlighting and support for PPHP files.",
    "version": "0.9.28",
    "repository": {},
    "publisher": "FenyEhabYar",
    "icon": "icons/pphp.png",
    "main": "./extension.js",
    "activationEvents": [
        "onLanguage:pphp"
    ],
    "galleryBanner": {
        "color": "#a31ec1"
    },
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "iconThemes": [
            {
                "id": "pphp-icons",
                "label": "PPHP Icons",
                "path": "./fileicons/pphp-icons-icon-theme.json"
            }
        ],
        "languages": [
            {
                "id": "pphp",
                "aliases": [
                    "PPHP",
                    "pphp"
                ],
                "extensions": [
                    ".pphp"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pphp",
                "scopeName": "source.pphp",
                "path": "./syntaxes/pphp.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "pphp",
                "path": "./snippets/pphp.json"
            }
        ]
    }
}